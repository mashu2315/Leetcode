35. Search Insert Position

class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int low = 0; int high = nums.size()-1;
        int mid, a=nums.size();
        if(target<nums[0]) return 0;
        while(low<=high){
           int mid = (low+high)/2;
            if(nums[mid]==target) return mid;
            else if(nums[mid]<target) {
                a = mid+1;
                low = mid+1;}
            else high = mid-1;
        } return a;
        
    }
};
